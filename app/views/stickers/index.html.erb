<h1>What Is Up!</h1>
<%= link_to "Add Sticker", new_sticker_path %>


<% @stickers.each do |sticker| %>
    <div>
        <%= image_tag sticker.image, alt: "#{sticker.name}" %>
        <p><%= sticker.name %></p>
        <%= link_to 'Edit', edit_sticker_path(sticker) %>
        <%= link_to 'Delete', sticker_path(sticker), method: :delete, data: {confirm: "Are You Sure?"} %>
    </div>
<% end %>

